Tab:AddToggle({
    Name = "Xero-AutoBounty",
    Default = false,
    Callback = function(Value)
        if Value then
            getgenv().Configs = {
                ["Team"] = "Pirates",
                ["Speed Tween"] = 350,
                ["Auto turn on race v3"] = true,
                ["Auto turn on race v4"] = true,
                ["Click Time"] = 0.2,
                ["White Screen"] = false,
                ["Chat"] = {["Enable"] = false, ["Message"] = {"ez"}},
                ["Run When Low Health"] = {["Enable"] = true, ["Health"] = 3500, ["Come back"] = true},
                ["Region"] = "Singapore",
                ["Weapons"] = {["Melee"] = {["Enable"] = true, ["Delay"] = 1.75, ["Order"] = 3}},
                ["Theme"] = {["Enable"] = true, ["Hide Theme"] = true, ["Name"] = "Gojo"},
                ["Webhook"] = {["Enable"] = false, ["Url"] = ""}
            }
            ExecuteScript("https://scripts.alchemyhub.xyz")
        end
    end      
})

