local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))() local Window = OrionLib:MakeWindow({Name = "Blox Fruits Script", HidePremium = false, SaveConfig = true, ConfigFolder = "BloxFruitsOrion"})

-- Tabs local Tab1 = Window:MakeTab({Name = "Farm", Icon = "rbxassetid://4483345998", PremiumOnly = false}) local Tab2 = Window:MakeTab({Name = "Quest/Item", Icon = "rbxassetid://4483345998", PremiumOnly = false}) local Tab3 = Window:MakeTab({Name = "Fruit and Raid", Icon = "rbxassetid://4483345998", PremiumOnly = false}) local Tab4 = Window:MakeTab({Name = "Teleport", Icon = "rbxassetid://4483345998", PremiumOnly = false}) local Tab5 = Window:MakeTab({Name = "Status", Icon = "rbxassetid://4483345998", PremiumOnly = false}) local Tab6 = Window:MakeTab({Name = "Visual", Icon = "rbxassetid://4483345998", PremiumOnly = false}) local Tab7 = Window:MakeTab({Name = "Shop", Icon = "rbxassetid://4483345998", PremiumOnly = false}) local Tab8 = Window:MakeTab({Name = "Misc", Icon = "rbxassetid://4483345998", PremiumOnly = false})

